<p id="notice"><%= notice %></p>

<h1>Reply Puntuations</h1>

<table>
  <thead>
    <tr>
      <th>Reply</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reply_puntuations.each do |reply_puntuation| %>
      <tr>
        <td><%= reply_puntuation.reply %></td>
        <td><%= reply_puntuation.user %></td>
        <td><%= link_to 'Show', reply_puntuation %></td>
        <td><%= link_to 'Edit', edit_reply_puntuation_path(reply_puntuation) %></td>
        <td><%= link_to 'Destroy', reply_puntuation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reply Puntuation', new_reply_puntuation_path %>
