  <thead>
    <table id="hnmain" border="0" cellpadding="0" align="center" cellspacing="0" width="85%" bgcolor="#f6f6ef"> 
    <tr>
      <td bgcolor="#ff6600">
        <table border="0" cellpadding="0" cellspacing="0" width="100%" >
          <tbody>
            <tr>
              <td style="width:18px;padding-right:4px">
                <a href="index.html.erb">
                  <img src="https://news.ycombinator.com/y18.gif" width="18" height="18" style="border:1px white solid;">
                </a>
              </td>
              <td style="line-height:12pt; height:10px;">
                <span class="pagetop">
                  <b class="hnname">
                    <%= link_to 'Hacker News', contributions_url %>
                  </b>
                  <%= link_to 'newest', contributions_url(:sort => "date")%> | <a href="">comments</a> | <a href="">show</a> | <a href="">ask</a> | <a href="">jobs</a> | <a href="contributions/new">submit</a> 
                </span>
              </td>
              <td style="text-align:right;padding-right:4px;">
                <span class="pagetop">
                  <a href="_form">login</a>
                </span>
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
    <tr style="height:10px"></tr>
    </table>
  </thead>
  <tbody border="0" cellpadding="0" cellspacing="0" class="itemlist">
   <% i=1 %>
   <% @contributions.each do |contribution| %>
     <table border="0" cellpadding="0" align="center" cellspacing="0" width="85%" bgcolor="#f6f6ef">
      <tr>
        <td class="enum"><%= i %>.<a href="#"><img width="10" height="10" src="heart.gif"></img></a></td>
        <td class="title">
          <a> <%= contribution.title %></a>
          <% if contribution.url!=nil %>
          <span>
            <a class="url_post" href="<%=contribution.url %>">(<%= contribution.url %>)</a>
          </span>
          <%end%>
        </td>
      </tr>
      <tr>
        <td></td>
        <td class="details">
          <span>
            <%= contribution.puntuation %> points
          </span>
          <a href="#">
            <span>
              by <%= contribution.user.username %>
            </span>
          </a>
          <a href="#">
            <span>
              <%= contribution.created_at %>
            </span>
          </a>
          <span>
             | 
          </span>
          <a href="#">
            <span>
              hide
            </span>
          </a>
          <span>
             | 
          </span>
          <a href="#">
            <span>
              0 comments
            </span>
          </a>
        </td>
      </tr>
      <tr class="spacer" style="height:5px"></tr>
      </table>
    <% i += 1 %>
    <% end %>
  </tbody>

<br>
